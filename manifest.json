{
  "key": "",
  "name": "feed.back",
  "version": "0.1",
  "manifest_version": 2,
  "description": "POC reader for news articles",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["firebase.js", "background.js"]
  },
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery-3.1.1.min.js", "firebase.js", "run.js"]
      }
    ],
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "permissions": ["identity", "identity.email", "tabs", "<all_urls>"],
  "oauth2": {
	"client_id": "17295082044-paq2uf4r9nv5nbougm3ctnip1pbl1e72.apps.googleusercontent.com",
	"scopes": ["profile"]
  },
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'"
}