{
  "key": "",
  "name": "feed.back",
  "version": "0.1",
  "manifest_version": 2,
  "description": "POC reader for news articles",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["identity", "identity.email", "tabs", "<all_urls>"]
}