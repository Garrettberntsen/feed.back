{
  "manifest_version": 2,

  "name": "News tracking extension",
  "description": "This extension will read a page and strip out news information",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "background",
    "browsingData",
    "history",
    "idle",
    "notifications",
    "tabs",
    "unlimitedStorage",
    "https://ajax.googleapis.com/"
  ]
}